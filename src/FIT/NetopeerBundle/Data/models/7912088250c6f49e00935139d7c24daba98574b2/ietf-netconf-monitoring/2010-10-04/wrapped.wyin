 </rnc> <common-counters eltype="grouping" config="true" description="Counters that exist both per session, and also globally,
accumulated from all sessions." iskey="false">
  <in-rpcs eltype="leaf" config="true" type="yang:zero-based-counter32" description="Number of correct &lt;rpc&gt; messages received." iskey="false"/>
  <in-bad-rpcs eltype="leaf" config="true" type="yang:zero-based-counter32" description="Number of messages received when an &lt;rpc&gt; message was expected,
